            "gh pr view": "allow",
            "gh pr create": "allow",
            "gh pr merge": "allow",
            "gh pr list": "allow",
            "git diff": "allow",
            "git log": "allow",
            "git stash": "allow",
            "git pull": "allow",
            "git ls-tree": "allow",
            "git commit": "deny",
            "grep": "allow",
            "head": "allow",
            "tail": "allow",
            "jq": "allow",
            "rg": "allow",
            "ls": "allow",
            "mkdir": "allow",
            "mv": "allow",
